{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Ubiquity": {
    "DefaultTimeout": 30000,
    "MaxRetries": 3,
    "ConnectionPoolSize": 10,
    "HealthCheckInterval": 60000
  },
  "Cors": {
    "AllowedOrigins": ["http://localhost:5000", "http://localhost:3000"]
  }
}
